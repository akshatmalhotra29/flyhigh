<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="flyhigh.flyhigh_ui.controller.Vendor"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:c="flyhigh.flyhigh_ui.util">
	<Page title="Vendor">
		<content>
			<l:Grid position="Center" id="gridContainer">
				<c:VBSpan span="3"/>
				<c:VBSpan span="6">
					<Title id="welcomeTitle"/>
					<GenericTile header="Catalog" class="sapUiTinyMargin fullWidth" subheader="Browse your Catalog" press="onPressCatalog">
						<TileContent>
							<Label id="catalogCount"/>
							<Label id="discountCount"/>
						</TileContent>
					</GenericTile>
					<GenericTile header="TBD" class="sapUiTinyMargin fullWidth" subheader="TBD" press="onPressVendor"/>
				</c:VBSpan>
				<c:VBSpan/>
			</l:Grid>
		</content>
	</Page>
</mvc:View>