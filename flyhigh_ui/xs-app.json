{
	"welcomeFile": "/webapp/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/webapp/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"scope": "$XSAPPNAME.USER"
	}, {
		"source": "/api",
		"authenticationType": "xsuaa",
		"destination": "flyhigh_node_api",
		"scope": "$XSAPPNAME.USER"
	}]
}